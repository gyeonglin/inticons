{
  "name": "inticons",
  "version": "0.4.37",
  "description": "Icons library and toolkit built by INTEGRATION Corp.",
  "keywords": [
    "font",
    "icon",
    "webfont"
  ],
  "homepage": "https://inticons.com",
  "author": "INTEGRATION Corp.",
  "contributors": [
    "Hyeonsu Kim (https://github.com/garudanish)",
    "Hyungwon Jang (https://github.com/HyungwonJang0327)",
    "Hansol Kim (https://github.com/gliburch)",
    "Bohyeon Seo (https://github.com/ktseo41)"
  ],
  "license": "MIT",
  "main": "fonts/inticons.bundle.min.css",
  "scripts": {
    "lint": "eslint .",
    "fontello": "node ./build/fontello && rm -rf .fontello-session && fontello-cli install --config ./fonts/config.json --css ./fonts/css --font ./fonts/font",
    "build-font": "node-sass ./fonts/scss/main.scss ./fonts/inticons.bundle.css && node-sass --output-style compressed ./fonts/scss/main.scss ./fonts/inticons.bundle.min.css",
    "build-docs": "node ./build/build-docs && cd ./docs && npm run build && cd ..",
    "build": "npm run fontello && npm run build-font && npm run build-docs",
    "serve": "cd ./docs && npm run serve && cd ..",
    "start": "npm run serve"
  },
  "files": [
    "fonts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/medistream-team/inticons"
  },
  "dependencies": {
    "fontello-cli": "^0.6.2",
    "fs-extra": "^10.1.0",
    "node-sass": "^7.0.1",
    "prettier": "^2.6.2",
    "svgo": "^2.8.0",
    "svgpath": "^2.5.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3"
  }
}
