{
  "name": "inticon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve docs/src/main.js",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "sass": "node-sass fonts/scss/main.scss ./style.css"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "core-js": "^3.8.3",
    "simple-syntax-highlighter": "^2.2.0",
    "vue": "^3.2.13",
    "vue-final-modal": "^3.4.4",
    "vue-loader": "^17.0.0",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "fontello-cli": "^0.6.2",
    "highlight.js": "^11.5.1",
    "marked": "^4.0.16",
    "node-sass": "^7.0.1",
    "prettier": "^2.6.2",
    "sass": "^1.52.1",
    "sass-loader": "^13.0.0",
    "svgpath": "^2.5.0",
    "webpack-chain": "^6.5.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/max-attributes-per-line": "off",
      "vue/html-self-closing": [
        "error",
        {
          "html": {
            "void": "always",
            "normal": "never",
            "component": "always"
          },
          "svg": "always",
          "math": "always"
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "semi": true,
          "tabWidth": 2,
          "arrowParens": "avoid",
          "endOfLine": "lf"
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
